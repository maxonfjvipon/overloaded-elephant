{
  "name": "maxonfjvipon/overloaded-elephant",
  "description": "php methods overloading",
  "license": "MIT",
  "authors": [
    {
      "name": "Max Trunnikov",
      "email": "mtrunnikov@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.13"
  },
  "autoload": {
    "psr-4": {
      "Maxonfjvipon\\OverloadedElephant\\": "src"
    }
  },
  "autoload-dev": {
    "Maxonfjvipon\\OverloadedElephant\\Tests\\": "tests"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    }
  }
}